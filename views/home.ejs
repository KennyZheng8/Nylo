<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Nylo</title>
    <link rel="stylesheet" href="style.css" />
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
  </head>
  <body>
    <main>
      <section class="glass">
        <div class="dashboard">
          <div class="title">
            <img src="image/full_logo.png" alt="logo"></img>
            <hr>
          </div>
          <div id="entryBox">
            <input id="roomId" type="text" placeholder="Room Name">
            <button id="createBotton" onclick="joinRoom()">Create New Room</button>
          </div>
        </div>
      </section> 
    </main>
    <!-- <div class="circle1"></div>
    <div class="circle2"></div> -->
    <script src="./script.js"></script>
    <script>
      function joinRoom() {
        if ($("#roomId").val() == '') {
          roomName = generateRoomName();
          window.open("Nylo/" + roomName, "_self");
        } else {
          window.open("Nylo/" + $("#roomId").val(), "_self");
        }
      }
    </script>
  </body>
</html>